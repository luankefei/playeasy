
#fs-view .tabbar 
    background: #ff6f76
    height: 50px

#fs-view .tabbar a 
    color: #fff
    display: inline-block
    font-size: 14px
    height: 32px
    line-height: 32px
    margin-top: 9px
    text-align: center
    width: 100px

#fs-view .tabbar a.selected 
    outline: 1px solid #fff

#fs-view .tabbar a:hover 
    background: #055747

/* 从tueasy页面中分离出来的css 2014.09.17 */
.waterfall li 
    float: left
    width: 180px
	height: 240px
    margin-right: 20px
    margin-bottom: 20px
    border: 1px solid #c9c9c6
    padding: 12px

.top 
    height: 120px
    margin-bottom: 40px

.top > .info 
    height: 90px

.top > .info > span 
    font-size: 20px
    line-height: 90px
    margin-right: 50px

.top > .search 
    height: 30px

.document > img 
    width: 100%
	height: 227px
    display: block

.document > .bottom 
    height: 30px
    padding-top: 10px

.document > .bottom > span 
    float: left
    height: 100%
    width: 140px 

.document > .bottom > i 
    float: right
    width: 20px
    height: 20px
    background: blue
    margin-top: 8px

.search-container * 
    float: left

.search-container > input 
    width: 200px

.search-container > button 
    width: 100px

#search-submit 
    margin-left: 20px
